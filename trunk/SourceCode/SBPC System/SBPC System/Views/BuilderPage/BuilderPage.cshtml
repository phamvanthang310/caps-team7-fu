<style>
    #demo-bar {
        z-index: 1000;
    }

    .jx-bar-rounded-tr {
    }

    .jx-bar-rounded-tl {
        border-top-left-radius: 5px;
    }

    .jx-bar {
        height: 30px;
        padding: 0px;
        width: 90%;
        background-color: #E5E5E5;
        border: 1px solid #B5B5B5;
    }

    .jx-bar-button ul {
        margin: 0px;
        padding: 0px;
    }

    ul, ol {
        margin: 0px 0px 10px 30px;
    }

    .jx-bar-button li {
        font-family: Verdana,Arial,Helvetica,sans-serif;
        font-size: 11px;
        cursor: pointer;
        padding: 4px 8px;
        border: 1px solid #E5E5E5;
        margin: 2px;
    }

    .jx-bar-button li {
        float: left;
        list-style: outside none none;
    }

        .jx-bar-button li a:link, .jx-bar-button li a:visited {
            font-family: Verdana,Arial,Helvetica,sans-serif;
            font-size: 11px;
            color: #666;
            text-decoration: none;
            padding: 1px;
        }

    .pt-smsquare div {
        background: url("~/Assets/img/layout-construction.png") repeat scroll 0px -350px transparent;
    }

    #pt-smsquare-tall-header div {
        background: url("../sprites/editor/page-manager/layout-sc884a25914.png") repeat scroll 0px 0px transparent;
    }

    .w-ui #pageManagerLayout .pt-smsquare div {
        height: 95px;
        width: 133px;
    }

    .pt-smsquare-tall-header1 div {
        background-position: -18px -202px;
    }

    .pt-smsquare div {
        position: relative;
        top: 8px;
        left: 8px;
        width: 100px;
        height: 101px;
        background: url("/weebly/images/page_type_squares_small.gif") no-repeat scroll 0% 0% transparent;
    }

    .navigation {
        background: none repeat scroll 0% 0% #CBB88E;
        min-height: 80px;
    }

    .navigation-detail {
        margin: 0px auto;
        width: 960px;
    }

    #header {
        border-collapse: collapse;
        border-spacing: 0px;
        width: 100%;
        height: 80px;
    }

        #header td {
            vertical-align: middle;
            text-align: left;
        }

    #logo {
        font-size: 36px;
        line-height: 1;
        color: #FFF;
        text-transform: uppercase;
        text-decoration: none;
        max-width: 400px;
    }

    #nav {
        min-width: 550px;
        max-width: 760px;
    }

    .wsite-logo {
        display: inline-block;
        vertical-align: top;
    }

    .wsite-title-placeholder {
        width: 10px;
        overflow: hidden;
        padding: 0px !important;
        margin-left: -10px !important;
    }

    .wsite-logo img, .wsite-logo span {
        vertical-align: top !important;
    }

    .wsite-title-placeholder {
        padding-right: 60px !important;
        cursor: default;
    }

    #nav ul {
        list-style: outside none none;
        float: left;
        margin: 0px;
        padding: 0px;
    }

        #nav ul li {
            list-style: outside none none;
            float: left;
            padding: 10px 10px 10px 15px;
        }

            #nav ul li a {
                display: block;
                font-weight: bold;
                color: #4A4A4A;
                text-decoration: none;
                padding: 5px 0px;
                border: 0px none;
                outline: 0px none;
                list-style-type: none;
                font-size: 14px;
                line-height: 1;
                text-transform: uppercase;
            }

                #nav ul li#active a, #nav ul li a:hover {
                    color: #FFF;
                }

    .container {
        min-height: 400px;
        padding: 55px 0px;
        margin: 0px auto;
        width: 960px;
    }

    #secondlistParent {
        position: relative;
    }

    .wsite-not-footer {
        text-align: left;
    }

    ul.weebly-content-area {
        position: relative !important;
        padding: 10px 0px !important;
        margin: -10px 0px !important;
        width: 100% !important;
    }

    .wsite-not-footer {
        text-align: left;
    }

    #empty-message {
        position: relative;
    }

    .empty-element-message {
        color: #CED0D2 !important;
        cursor: default;
        font-family: "Open Sans",Helvetica,Arial,Verdana,sans-serif;
        font-size: 20px;
        font-weight: 200;
        letter-spacing: 2px;
        text-transform: uppercase;
        -moz-user-select: none;
    }

    #empty-message-inner {
        background-repeat: no-repeat;
        background-position: center bottom;
        color: #CED0D2 !important;
        padding: 100px 0px 110px;
        background-image: url("../Assets/img/drag-instructions.png");
    }

    #empty-message-inner {
        padding: 100px 0px;
        width: 100%;
        text-align: center;
        font-size: 20px;
        color: #AAA;
    }
</style>
