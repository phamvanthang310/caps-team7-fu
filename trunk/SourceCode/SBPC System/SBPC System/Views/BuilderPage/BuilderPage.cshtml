@{
    ViewBag.Title = "Builder";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    .builder-element-container {
        background: none repeat scroll 0% 0% #F2F2F2;
        border-left: 1px solid #DDD;
        border-right: 0px none;
        position: fixed;
    }

    .builder-element-pick {
        padding: 11px 15px 11px 20px;
        background: none repeat scroll 0% 0% #FBFBFB;
        position: relative;
    }

    .holder-style:hover {
        background-color: white;
        outline-color: #70CA63;
    }

    .row {
        margin: 0;
    }

    .scroll {
        overflow-y: scroll;
        overflow-x: hidden;
        height: 100%;
    }

    #demo-bar {
        z-index: 1000;
    }

    .jx-bar-rounded-tr {
    }

    .jx-bar-rounded-tl {
        border-top-left-radius: 5px;
    }

    .jx-bar {
        height: 30px;
        padding: 0px;
        width: 50%;
        background-color: #E5E5E5;
        border: 1px solid #B5B5B5;
    }

    .jx-bar-button ul {
        margin: 0px;
        padding: 0px;
    }

    .jx-bar-button li {
        font-family: Verdana,Arial,Helvetica,sans-serif;
        font-size: 11px;
        cursor: pointer;
        padding: 4px 8px;
        border: 1px solid #1DA5D2;
        margin: 2px;
    }

    .jx-bar-button li {
        float: left;
        list-style: outside none none;
    }

        .jx-bar-button li a:link, .jx-bar-button li a:visited {
            font-family: Verdana,Arial,Helvetica,sans-serif;
            font-size: 11px;
            color: #666;
            text-decoration: none;
            padding: 1px;
        }

    .multiple-choice-box-selected .multiple-choice-box-inner-left, .multiple-choice-box-selected:hover .multiple-choice-box-inner-left {
        border-right: 1px solid #B0CEB7;
    }

    .multiple-choice-box-inner-left {
        float: left;
        width: 43px;
        border-right: 1px solid #E1E1E1;
        padding: 30px 0px;
        text-align: center;
    }

    .multiple-choice-box {
        position: relative;
        border: 1px solid #E1E1E1;
        border-radius: 3px;
        background: none repeat scroll 0% 0% #F6F7F9;
        margin: 10px 0px;
        text-shadow: none;
        cursor: pointer;
    }

    .multiple-choice-box-selected .multiple-choice-box-inner-right, .multiple-choice-box-selected:hover .multiple-choice-box-inner-right {
        border-left: 1px solid #F7FCF8;
    }

    .multiple-choice-box-inner-right {
        float: left;
        border-left: 1px solid #FFF;
        padding: 12px 20px 10px 13px;
        width: 615px;
    }

    .multiple-choice-box-title {
        font-weight: bold;
        font-size: 14px;
        margin-bottom: 7px;
        color: #333;
    }

    .multiple-choice-box-body {
        color: #666;
    }
</style>
